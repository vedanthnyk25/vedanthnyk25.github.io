#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/tsconfck@3.1.5_typescript@5.8.2/node_modules/tsconfck/bin/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/tsconfck@3.1.5_typescript@5.8.2/node_modules/tsconfck/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/tsconfck@3.1.5_typescript@5.8.2/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/tsconfck@3.1.5_typescript@5.8.2/node_modules/tsconfck/bin/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/tsconfck@3.1.5_typescript@5.8.2/node_modules/tsconfck/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/tsconfck@3.1.5_typescript@5.8.2/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsconfck/bin/tsconfck.js" "$@"
else
  exec node  "$basedir/../tsconfck/bin/tsconfck.js" "$@"
fi
