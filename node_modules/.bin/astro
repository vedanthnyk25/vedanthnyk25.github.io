#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/astro@5.5.4_jiti@2.4.2_lightningcss@1.29.2_rollup@4.37.0_typescript@5.8.2/node_modules/astro/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/astro@5.5.4_jiti@2.4.2_lightningcss@1.29.2_rollup@4.37.0_typescript@5.8.2/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/astro@5.5.4_jiti@2.4.2_lightningcss@1.29.2_rollup@4.37.0_typescript@5.8.2/node_modules/astro/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/astro@5.5.4_jiti@2.4.2_lightningcss@1.29.2_rollup@4.37.0_typescript@5.8.2/node_modules:/home/vedanthnyk25/Desktop/louai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
